{  
    "provider":"SAP Demo",
    "name":"Person API",
    "description":"Local Service for managing Persons",
    "api":{  
       "targetUrl":"https://personservice.personservice.svc.cluster.local",
       "spec":{  
          "swagger":"2.0",
          "info":{  
             "description":"API that allows to perform crud Operations for person information stored in Mongo DB. It also includes an endpoint to display all environment variables and their respective values in the capplication Container. In further evolutions of the code extensions to Manage the Kyma registration are added.",
             "version":"1.0",
             "title":"Person Service Kubernetes API",
             "contact":{  
                "name":"Andreas Krause",
                "email":"and.krause@sap.com"
             },
             "license":{  
 
             }
          },
          "host":"localhost:8080",
          "basePath":"/",
          "tags":[  
             {  
                "name":"application-connector-api",
                "description":"Application Connector Api"
             },
             {  
                "name":"environment-api",
                "description":"Environment Api"
             },
             {  
                "name":"person-api",
                "description":"Person Api"
             }
          ],
          "paths":{  
             "/api/v1/applicationconnector/registration":{  
                "get":{  
                   "tags":[  
                      "application-connector-api"
                   ],
                   "summary":"Read Kyma Environment registrations",
                   "description":"This Operation reads all Environment registration information.",
                   "operationId":"readServiceRegistrationsUsingGET",
                   "produces":[  
                      "*/*"
                   ],
                   "responses":{  
                      "200":{  
                         "description":"OK",
                         "schema":{  
                            "type":"array",
                            "items":{  
                               "$ref":"#/definitions/RegistrationQueryResponse"
                            }
                         }
                      }
                   },
                   "deprecated":false
                },
                "post":{  
                   "tags":[  
                      "application-connector-api"
                   ],
                   "summary":"Register to Kyma Environment",
                   "description":"This Operation registers the Service to the configured Kyma environment",
                   "operationId":"serviceRegistrationUsingPOST",
                   "consumes":[  
                      "application/json"
                   ],
                   "produces":[  
                      "*/*"
                   ],
                   "responses":{  
                      "200":{  
                         "description":"OK",
                         "schema":{  
                            "type":"object",
                            "additionalProperties":{  
                               "type":"string"
                            }
                         }
                      }
                   },
                   "deprecated":false
                },
                "delete":{  
                   "tags":[  
                      "application-connector-api"
                   ],
                   "summary":"Delete Kyma Environment registrations",
                   "description":"This Operation deletes all Environment registration information.",
                   "operationId":"deleteServiceRegistrationUsingDELETE",
                   "produces":[  
                      "*/*"
                   ],
                   "responses":{  
                      "200":{  
                         "description":"OK",
                         "schema":{  
                            "type":"string"
                         }
                      }
                   },
                   "deprecated":false
                }
             },
             "/api/v1/environment":{  
                "get":{  
                   "tags":[  
                      "environment-api"
                   ],
                   "summary":"List Environment Variables",
                   "description":"This Operation retrieves a list of all environment variables available in the underlying environment (e.g. container).",
                   "operationId":"getEnvironmentUsingGET",
                   "produces":[  
                      "*/*"
                   ],
                   "responses":{  
                      "200":{  
                         "description":"OK",
                         "schema":{  
                            "type":"object",
                            "additionalProperties":{  
                               "type":"string"
                            }
                         }
                      }
                   },
                   "deprecated":false
                }
             },
             "/api/v1/person":{  
                "get":{  
                   "tags":[  
                      "person-api"
                   ],
                   "summary":"Get Person List",
                   "description":"This function retrieves all persons from the Database.",
                   "operationId":"findAllUsingGET",
                   "produces":[  
                      "*/*"
                   ],
                   "responses":{  
                      "200":{  
                         "description":"OK",
                         "schema":{  
                            "$ref":"#/definitions/Iterable�Person�"
                         }
                      }
                   },
                   "deprecated":false
                },
                "post":{  
                   "tags":[  
                      "person-api"
                   ],
                   "summary":"Create Person",
                   "description":"This function creates a new person entry on the DB.Externally supplied ids are ignored.",
                   "operationId":"createUsingPOST",
                   "consumes":[  
                      "application/json"
                   ],
                   "produces":[  
                      "*/*"
                   ],
                   "parameters":[  
                      {  
                         "in":"body",
                         "name":"person",
                         "description":"person",
                         "required":true,
                         "schema":{  
                            "$ref":"#/definitions/Person"
                         }
                      }
                   ],
                   "responses":{  
                      "200":{  
                         "description":"OK",
                         "schema":{  
                            "$ref":"#/definitions/Person"
                         }
                      }
                   },
                   "deprecated":false
                },
                "delete":{  
                   "tags":[  
                      "person-api"
                   ],
                   "summary":"Delete all Persons in DB",
                   "description":"This function deletes all persons from the database.",
                   "operationId":"deleteAllUsingDELETE",
                   "produces":[  
                      "*/*"
                   ],
                   "responses":{  
                      "200":{  
                         "description":"OK",
                         "schema":{  
                            "$ref":"#/definitions/Person"
                         }
                      }
                   },
                   "deprecated":false
                }
             },
             "/api/v1/person/search":{  
                "post":{  
                   "tags":[  
                      "person-api"
                   ],
                   "summary":"Search for People",
                   "description":"This function performs a people search in the database, and returns the persons that meet the criteria specified in the request.",
                   "operationId":"searchUsingPOST",
                   "consumes":[  
                      "application/json"
                   ],
                   "produces":[  
                      "*/*"
                   ],
                   "parameters":[  
                      {  
                         "in":"body",
                         "name":"person",
                         "description":"person",
                         "required":true,
                         "schema":{  
                            "$ref":"#/definitions/Person"
                         }
                      }
                   ],
                   "responses":{  
                      "200":{  
                         "description":"OK",
                         "schema":{  
                            "$ref":"#/definitions/Iterable�Person�"
                         }
                      }
                   },
                   "deprecated":false
                }
             },
             "/api/v1/person/{id}":{  
                "get":{  
                   "tags":[  
                      "person-api"
                   ],
                   "summary":"Read Person by ID",
                   "description":"This function reads a single person from the database.",
                   "operationId":"readUsingGET",
                   "produces":[  
                      "*/*"
                   ],
                   "parameters":[  
                      {  
                         "name":"id",
                         "in":"path",
                         "description":"id",
                         "required":true,
                         "type":"string"
                      }
                   ],
                   "responses":{  
                      "200":{  
                         "description":"OK",
                         "schema":{  
                            "$ref":"#/definitions/Person"
                         }
                      }
                   },
                   "deprecated":false
                },
                "delete":{  
                   "tags":[  
                      "person-api"
                   ],
                   "summary":"Delete Person by ID",
                   "description":"This function deletes a person from the database.",
                   "operationId":"deleteUsingDELETE",
                   "produces":[  
                      "*/*"
                   ],
                   "parameters":[  
                      {  
                         "name":"id",
                         "in":"path",
                         "description":"id",
                         "required":true,
                         "type":"string"
                      }
                   ],
                   "responses":{  
                      "200":{  
                         "description":"OK",
                         "schema":{  
                            "$ref":"#/definitions/Person"
                         }
                      }
                   },
                   "deprecated":false
                },
                "patch":{  
                   "tags":[  
                      "person-api"
                   ],
                   "summary":"Update Person",
                   "description":"This function updates a person from on the database. This does not support delta updates.",
                   "operationId":"upsertUsingPATCH",
                   "consumes":[  
                      "application/json"
                   ],
                   "produces":[  
                      "*/*"
                   ],
                   "parameters":[  
                      {  
                         "name":"id",
                         "in":"path",
                         "description":"id",
                         "required":true,
                         "type":"string"
                      },
                      {  
                         "in":"body",
                         "name":"person",
                         "description":"person",
                         "required":true,
                         "schema":{  
                            "$ref":"#/definitions/Person"
                         }
                      }
                   ],
                   "responses":{  
                      "200":{  
                         "description":"OK",
                         "schema":{  
                            "$ref":"#/definitions/Person"
                         }
                      }
                   },
                   "deprecated":false
                }
             }
          },
          "definitions":{  
             "Iterable�Person�":{  
                "type":"object",
                "title":"Iterable�Person�"
             },
             "Person":{  
                "type":"object",
                "properties":{  
                   "city":{  
                      "type":"string",
                      "example":"Muenchen",
                      "description":"City Name"
                   },
                   "extensionFields":{  
                      "type":"object",
                      "description":"Arbitrary json key value pairs"
                   },
                   "firstName":{  
                      "type":"string",
                      "example":"John",
                      "description":"First Name of the Person"
                   },
                   "houseNumber":{  
                      "type":"string",
                      "example":86,
                      "description":"House Number"
                   },
                   "id":{  
                      "type":"string",
                      "example":"1bf8b88a-7bb8-4b92-90bc-d1fa34a60a57",
                      "description":"Unique MongoDB identifier"
                   },
                   "lastName":{  
                      "type":"string",
                      "example":"Doe",
                      "description":"Last Name of the Person"
                   },
                   "streetAddress":{  
                      "type":"string",
                      "example":"Nymphenburger Str.",
                      "description":"Street name w/o house number"
                   },
                   "zip":{  
                      "type":"string",
                      "example":80636,
                      "description":"House Number"
                   }
                },
                "title":"Person"
             },
             "RegistrationQueryResponse":{  
                "type":"object",
                "properties":{  
                   "description":{  
                      "type":"string"
                   },
                   "id":{  
                      "type":"string"
                   },
                   "name":{  
                      "type":"string"
                   },
                   "provider":{  
                      "type":"string"
                   }
                },
                "title":"RegistrationQueryResponse"
             }
          }
       }
    },
    "events":{  
       "spec":{  
          "asyncapi":"1.0.0",
          "info":{  
             "title":"personservicekubernetes-events",
             "version":"v1",
             "description":"PSK Events v1"
          },
          "topics":{  
             "person.created.v1":{  
                "subscribe":{  
                   "summary":"Person Created",
                   "payload":{  
                      "type":"object",
                      "required":[  
                         "personid"
                      ],
                      "properties":{  
                         "personid":{  
                            "type":"string",
                            "example":{  
                               "personid":"1bf8b88a-7bb8-4b92-90bc-d1fa34a60a57"
                            },
                            "properties":null,
                            "description":"ID of the Person's database record'",
                            "title":"ID"
                         }
                      }
                   }
                }
             },
             "person.changed.v1":{  
                "subscribe":{  
                   "summary":"Person Changed",
                   "payload":{  
                      "type":"object",
                      "required":[  
                         "personid"
                      ],
                      "properties":{  
                         "personid":{  
                            "type":"string",
                            "example":{  
                               "personid":"1bf8b88a-7bb8-4b92-90bc-d1fa34a60a57"
                            },
                            "properties":null,
                            "description":"ID of the Person's database record'",
                            "title":"ID"
                         }
                      }
                   }
                }
             },
             "person.deleted.v1":{  
                "subscribe":{  
                   "summary":"Person Deleted",
                   "payload":{  
                      "type":"object",
                      "required":[  
                         "personid"
                      ],
                      "properties":{  
                         "personid":{  
                            "type":"string",
                            "example":{  
                               "personid":"1bf8b88a-7bb8-4b92-90bc-d1fa34a60a57"
                            },
                            "properties":null,
                            "description":"ID of the Person's database record'",
                            "title":"ID"
                         }
                      }
                   }
                }
             }
          }
       }
    },
    "documentation":{  
       "displayName":"Personservicekubernetes",
       "description":"Persosnservicekubernetes",
       "type":"Personservicekubernetes",
       "tags":[  
          "Personservicekubernetes"
       ],
       "docs":[  
          {  
             "title":"Documentation",
             "type":"Doc",
             "source":"https://google.com"
          }
       ]
    }
 }